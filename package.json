{
  "name": "somehow-graph",
  "description": "make infographics without thinking",
  "version": "0.3.6",
  "main": "src/index.js",
  "unpkg": "builds/somehow.min.js",
  "module": "builds/somehow.mjs",
  "author": "Spencer Kelly (spencermountain)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencermountain/somehow.git"
  },
  "homepage": "https://github.com/spencermountain/somehow#readme",
  "scripts": {
    "start": "budo scratch.js:assets/bundle.js --live",
    "watch": "npm run start",
    "version": "node ./scripts/version.js",
    "filesize": "node ./scripts/filesize.js",
    "build": "npm run version && rollup -c && npm run filesize",
    "build:demo": "browserify scratch.js -t [ babelify --presets [ @babel/preset-env ] ] | derequire > ./assets/bundle.js"
  },
  "files": [
    "src",
    "builds",
    "_version.js"
  ],
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "dependencies": {
    "d3-shape": "1.3.7",
    "fit-aspect-ratio": "2.1.0",
    "htm": "2.2.1",
    "somehow-ticks": "^0.0.3",
    "spacetime": "6.3.0",
    "spacetime-ticks": "0.2.1",
    "spencer-color": "^0.1.0",
    "vhtml": "2.2.0"
  },
  "devDependencies": {
    "derequire": "^2.0.6",
    "babelify": "^10.0.0",
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "budo": "^11.6.3",
    "rollup": "1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.2.0"
  }
}
